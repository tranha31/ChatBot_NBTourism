        <tenDiaDiem rdf:datatype="http://www.w3.org/2001/XMLSchema#string">xã Ninh Hải, huyện Hoa Lư, tỉnh Ninh Bình</tenDiaDiem>
        <thuocHuyen rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hoa Lư</thuocHuyen>
