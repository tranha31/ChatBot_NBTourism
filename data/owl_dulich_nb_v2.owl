    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/taha/ontologies/2021/10/untitled-ontology-3#DL24LC">
        <rdf:type rdf:resource="http://www.semanticweb.org/taha/ontologies/2021/10/untitled-ontology-3#DiaDiem"/>
